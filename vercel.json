
{
  "functions": {
    "pages/api/auth/index.js": {
      "runtime": "nodejs18.x"
    },
    "pages/api/auth/callback.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "/api/:match*"
    },
    {
      "source": "/blog-admin/(.*)",
      "destination": "/blog-admin/$1"
    }
  ]
}
