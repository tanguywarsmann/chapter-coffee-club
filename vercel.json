{
  "redirects": [
    { "source": "/a-propos/index.html", "destination": "/a-propos", "permanent": true },
    { "source": "/presse/index.html", "destination": "/presse", "permanent": true },
    { "source": "/Ã -propos", "destination": "/a-propos", "permanent": true },
    { "source": "/apropos", "destination": "/a-propos", "permanent": true },
    { "source": "/about", "destination": "/a-propos", "permanent": true },
    { "source": "/press", "destination": "/presse", "permanent": true },
    {
      "source": "/(.*)",
      "destination": "https://www.vread.fr/$1",
      "permanent": true,
      "has": [{ "type": "host", "value": "vread.fr" }]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; img-src 'self' data: https:; media-src 'self' data:; style-src 'self' 'unsafe-inline' https:; font-src 'self' data: https:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https:; connect-src 'self' https:; base-uri 'self'; form-action 'self'; frame-ancestors 'none'"
        },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Strict-Transport-Security", "value": "max-age=63072000; includeSubDomains; preload" }
      ]
    }
  ]
}
