
{
  "functions": {
    "api/auth/index.js": {
      "runtime": "nodejs20.x"
    },
    "api/auth/callback.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/auth",
      "destination": "/api/auth"
    },
    {
      "source": "/api/auth/callback",
      "destination": "/api/auth/callback"
    },
    {
      "source": "/blog-admin/(.*)",
      "destination": "/blog-admin/$1"
    }
  ]
}
